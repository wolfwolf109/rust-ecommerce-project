{
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "features": {
    "ghcr.io/devcontainers/features/rust:1": {}
  },
  "hostRequirements": {
    "cpus": 2
  },
  "portsAttributes": {
    "8888": {
      "label": "Application",
      "onAutoForward": "openPreview"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "rust-lang.rust-analyzer"
      ]
    }
  },
  "forwardPorts": [8888]
}
